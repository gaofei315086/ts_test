{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {},
    "overrides":[//要配置具体文件规则overrides
        {
            "files": ["*.ts"],
            "extends": [
            ],
            "rules": {   //配置具体检测规则
                "no-console":"off"
            }
        }
    ]
}
